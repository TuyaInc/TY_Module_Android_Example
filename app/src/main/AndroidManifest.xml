<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuya.example">

    <application
        android:name="com.tuya.smart.framework.LauncherApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="APPLICATION_AGENT" android:value="com.tuya.example.appstart.ApplicationAgentExample"/>
        <meta-data android:name="ACTIVITY_AGENT" android:value="com.tuya.example.appstart.LauncherActivityAgentExample"/>
        <activity android:name="com.tuya.smart.framework.LauncherActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>

</manifest>
